import std.io;

i32 foo() {
  // no return, should return 0
}

i32 bar() {
    // no return, should return 0
}

void baz() {
    // no return, should return void
}

void main() {
  i32 x = foo();
  if (x != 0) {
      printf("Error: foo returned %d\n", x);
  }
  i32 y = bar();
  if (y != 0) {
      printf("Error: bar returned %d\n", y);
  }
  baz();
  printf("Success\n");
}
