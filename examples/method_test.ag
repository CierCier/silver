// Simple test for struct methods
import std.io;

struct Point {
    i32 x;
    i32 y;
}

impl Point {
    // Static method (no self)
    Point new(i32 x, i32 y) {
        Point p;
        p.x = x;
        p.y = y;
        return p;
    }

    // Instance method (has self)
    void print(Point self) {
        printf("Point(%d, %d)\n", self.x, self.y);
    }
}

i32 main() {
    // Test static method call
    Point p = Point.new(10, 20);
    
    // Test instance method call
    p.print();
    
    printf("Done!\n");
    return 0;
}
