import std.io;

struct Box<T> {
    T value;
}

struct Pair<K, V> {
    K key;
    V value;
}

// void foo<T>(T x) { }

i32 main() {
    Box<i32> b;
    b.value = 42;
    printf("Box value: %d\n", b.value);

    Pair<i32, f64> p;
    p.key = 10;
    p.value = 3.14;
    printf("Pair: %d, %f\n", p.key, p.value);

    return 0;
}
