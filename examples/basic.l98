

declare printf(fmt: string, args: ...): i32;

fn factorial(n: i32) : i32 {

    if (n == 0){
        return 1;
    }else{
        return n * factorial(n - 1);
    }

    // This line is unreachable
    // but needs to be here to make the function complete
    return 0;
}

fn main(): i32 {
    let n:i32 = 5;

    printf("Factorial of %d is %d\n", n,  factorial(n) );

    return 0;
}